{
  "name": "@sanity/react-hooks",
  "version": "1.149.19",
  "description": "Officially supported Sanity Studio API building blocks",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Sanity.io <hello@sanity.io>",
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "run-s build:js build:assets",
    "build:assets": "cpx 'src/**/*.{css,json}' lib",
    "build:js": "babel src --out-dir lib --extensions '.js,.ts,.tsx'",
    "clean": "rimraf coverage lib",
    "prebuild": "run-s clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "react-hooks",
    "structure",
    "api"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sanity/base": "1.149.19",
    "lodash": "^4.17.15",
    "rxjs": "^6.5.3",
    "shallow-equals": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "jest": "^24.9.0",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "bugs": {
