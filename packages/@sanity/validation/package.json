{
  "name": "@sanity/validation",
  "version": "1.149.16",
  "description": "Validation and warning infrastructure for Sanity projects",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:assets": "cpx 'src/**/*.{css,json}' lib",
    "build:js": "babel src --out-dir lib --extensions '.js,.ts,.tsx'",
    "clean": "rimraf lib",
    "prebuild": "run-s clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "run-p watch:*",
    "watch:assets": "yarn build:assets --watch",
    "watch:js": "yarn build:js --watch"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "validation"
  ],
  "homepage": "https://www.sanity.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "devDependencies": {
    "jest": "^24.9.0"
  },
  "dependencies": {
    "clone-deep": "^2.0.1",
    "date-fns": "^1.29.0",
    "es6-error": "^4.0.2",
    "type-of-is": "^3.5.1"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testURL": "http://localhost/"
  }
}
