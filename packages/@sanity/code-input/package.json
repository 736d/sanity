{
  "name": "@sanity/code-input",
  "version": "1.149.16",
  "description": "Ace editor for editing code",
  "main": "lib/index.js",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:assets": "cpx 'src/**/*.{css,json}' lib",
    "build:js": "babel src --out-dir lib --extensions '.js,.ts,.tsx'",
    "clean": "rimraf lib",
    "prebuild": "run-s clean",
    "prepublishOnly": "sanity-check",
    "watch": "run-p watch:*",
    "watch:assets": "yarn build:assets --watch",
    "watch:js": "yarn build:js --watch"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "code-input",
    "sanity-plugin",
    "code-editor"
  ],
  "dependencies": {
    "lodash": "^4.17.15",
    "react-ace": "^5.0.1",
    "react-icons": "^2.2.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@sanity/check": "1.149.16",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  }
}
