{
  "name": "@sanity/preview",
  "version": "1.149.19",
  "description": "Utilities for previewing content in Sanity",
  "main": "index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:assets": "cpx 'src/**/*.{css,json}' lib",
    "build:js": "babel src --out-dir lib --extensions '.js,.ts,.tsx'",
    "clean": "rimraf lib",
    "prebuild": "run-s clean",
    "test": "#todo: jest",
    "test:watch": "jest --watchAll",
    "watch": "run-p watch:*",
    "watch:assets": "yarn build:assets --watch",
    "watch:js": "yarn build:js --watch"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "preview"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "dependencies": {
    "@sanity/image-url": "^0.140.19",
    "lodash": "^4.17.15",
    "observable-props": "^2.0.0",
    "react-props-stream": "^1.0.0",
    "rxjs": "^6.5.3",
    "shallow-equals": "^1.0.0"
  },
  "devDependencies": {
    "empty": "^0.10.1",
    "jest": "^24.9.0",
    "typescript-plugin-css-modules": "^1.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "prop-types": "^15.6 || ^16",
    "react": "^16.9"
  },
  "homepage": "https://www.sanity.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  }
}
