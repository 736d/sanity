{
  "name": "@sanity/default-layout",
  "version": "1.149.19",
  "description": "The default layout components for Sanity",
  "main": "lib/default-layout.js",
  "scripts": {
    "build": "run-s build:*",
    "build:assets": "cpx 'src/**/*.{css,json}' lib",
    "build:js": "babel src --out-dir lib --extensions '.js,.ts,.tsx'",
    "clean": "rimraf lib .nyc_output coverage",
    "coverage": "nyc --report=lcov --report=text _mocha -- test/**/*.test.js",
    "prebuild": "run-s clean",
    "prepublishOnly": "sanity-check",
    "watch": "run-p watch:*",
    "watch:assets": "yarn build:assets --watch",
    "watch:js": "yarn build:js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "default-layout",
    "layout"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "dependencies": {
    "@sanity/base": "1.149.19",
    "@sanity/generate-help-url": "1.149.16",
    "is-hotkey": "^0.1.4",
    "lodash": "^4.17.15",
    "react-click-outside": "^3.0.0",
    "react-ink": "^6.1.0",
    "react-props-stream": "^1.0.0",
    "react-tippy": "^1.2.3",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@sanity/check": "1.149.16",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cpx": "^1.5.0",
    "mocha": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^11.0.3",
    "prop-types": "^15.6.0",
    "rimraf": "^2.7.1"
  },
  "peerDependencies": {
    "@sanity/base": "^0.147.0",
    "prop-types": "^15.6 || ^16",
    "react": "^16.9",
    "react-dom": "^16.9"
  }
}
