{
  "name": "@sanity/presence-store",
  "version": "1.149.16",
  "description": "Client side presence channel handler",
  "main": "lib/PresenceStore.js",
  "scripts": {
    "build": "run-s build:*",
    "build:assets": "cpx 'src/**/*.{css,json}' lib",
    "build:js": "babel src --out-dir lib --extensions '.js,.ts,.tsx'",
    "clean": "rimraf lib",
    "prebuild": "run-s clean",
    "test": "NODE_ENV=test tape -r @babel/register test/*.test.js",
    "watch": "run-p watch:*",
    "watch:assets": "yarn build:assets --watch",
    "watch:js": "yarn build:js --watch"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^2.7.1",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "@sanity/uuid": "1.149.16",
    "deep-equal": "^1.0.1",
    "rxjs": "^6.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "presence-store"
  ],
  "homepage": "https://www.sanity.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  }
}
