    "@babel/core": "^7.11.1",
    "@sanity/schema": "1.149.19",
    "assert": "^1.4.1",
    "jest": "^24.9.0",
    "jsdom": "^12.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.2.0",
    "xpath": "0.0.27"
  },
  "directories": {
