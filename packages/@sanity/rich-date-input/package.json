{
  "name": "@sanity/rich-date-input",
  "version": "1.149.16",
  "description": "Date/time input components for Sanity form builder",
  "main": "lib/index.js",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:assets": "cpx 'src/**/*.{css,json}' lib",
    "build:js": "babel src --out-dir lib --extensions '.js,.ts,.tsx'",
    "clean": "rimraf lib",
    "prebuild": "run-s clean",
    "prepublishOnly": "sanity-check",
    "watch": "run-p watch:*",
    "watch:assets": "yarn build:assets --watch",
    "watch:js": "yarn build:js --watch"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "rich-date-input",
    "sanity-plugin"
  ],
  "dependencies": {
    "@sanity/generate-help-url": "1.149.16",
    "lodash": "^4.17.15",
    "moment": "^2.19.1",
    "moment-timezone": "^0.5.11",
    "prop-types": "^15.6.0",
    "react-datepicker": "^1.8.0"
  },
  "devDependencies": {
    "@sanity/check": "1.149.16",
    "rimraf": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/"
}
