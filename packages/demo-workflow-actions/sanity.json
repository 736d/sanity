{
  "root": true,
  "project": {
    "name": "Workflow demo"
  },
  "api": {
    "projectId": "m2begyfh",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/plugin-review-workflow"
  ],
  "env": {
    "development": {
      "plugins": ["@sanity/vision"]
    }
  },
  "parts": [
    {
      "implements": "part:@sanity/base/document-actions/resolver",
      "path": "app/documentActions"
    },
    {
      "implements": "part:@sanity/plugin-review-workflow/config",
      "path": "app/reviewWorkflow"
    },
    {
      "name": "part:@sanity/base/schema",
      "path": "app/schema"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "app/structure"
    }
  ]
}
